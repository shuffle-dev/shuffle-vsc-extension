{
    "name": "shuffle",
    "displayName": "shuffle-ext",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:shuffle.openWindow"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "keybindings": {
            "key": "ctrl+d",
            "command": "shuffle.openWindow"
        },
        "commands": [
            {
                "command": "shuffle.openWindow",
                "title": "Shuffle builder: Open shuffle builder window"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/node-fetch": "^2.5.7",
        "@types/vscode": "^1.50.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "node-fetch": "^2.6.1"
    }
}
